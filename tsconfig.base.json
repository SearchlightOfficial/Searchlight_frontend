{
  "compilerOptions": {
    "target": "es2017",
    "module": "ESNext",
    "lib": ["es2017", "dom"],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "rootDir": ".",
    "outDir": "./dist",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "incremental": true,
    "strictPropertyInitialization": false
  }
}
